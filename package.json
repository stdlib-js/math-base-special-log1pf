  "__stdlib__": {
    "scaffold": {
      "$schema": "math/base@v1.0",
      "base_alias": "log1p",
      "alias": "log1pf",
      "pkg_desc": "evaluate the natural logarithm of 1+x as a single-precision floating-point number",
      "desc": "evaluates the natural logarithm of 1+x as a single-precision floating-point number",
      "short_desc": "natural logarithm of 1+x",
      "parameters": [
        {
          "name": "x",
          "desc": "input value",
          "type": {
            "javascript": "number",
            "jsdoc": "number",
            "c": "float",
            "dtype": "float32"
          },
          "domain": [
            {
              "min": -1,
              "max": "infinity"
            }
          ],
          "rand": {
            "prng": "random/base/uniform",
            "parameters": [
              -0.5,
              10
            ]
          },
          "example_values": [
            4,
            0.5,
            0.1,
            0.01,
            0.001,
            1,
            2,
            3,
            5,
            10,
            0.2,
            0.3,
            0.7,
            1.5,
            2.5,
            7,
            -0.5,
            -0.1,
            -0.9,
            0
          ]
        }
      ],
      "returns": {
        "desc": "natural logarithm of 1+x",
        "type": {
          "javascript": "number",
          "jsdoc": "number",
          "c": "float",
          "dtype": "float32"
        }
      },
      "keywords": [
        "log1p",
        "logarithm",
        "ln",
        "natural"
      ],
      "extra_keywords": [
        "math.log",
        "math.log1p"
      ]
